function ExecutarRetorno(campo, novoValor) {
    var callback = eval('(' + campo.attr('data-cb') + ')');

    if (callback !== undefined) {

        if (novoValor !== undefined && callback.value == novoValor) {

            $('#' + callback.id).show();

        }
        else {

            $('#' + callback.id).hide();

        }
    }
}

function PreencherParametros(modulo) {
    
    var modulos = [];
    //Adicionar ao array de módulos a serem preenchidos o módulo pertinente, ou adicionar todos
    switch (modulo.toUpperCase()) {
        case 'ATIVO':
            modulos.push('Ativo');
            break;
        case 'COMERCIAL':
            modulos.push('Comercial');
            break;
        case 'COMISSOES':
            modulos.push('Comissoes');
            break;
        case 'CONTABIL':
            modulos.push('Contabil');
            break;
        case 'FINANCEIRO':
            modulos.push('Financeiro');
            break;
        case 'GENERICOS':
            modulos.push('Genericos');
            break;
        case 'MATERIAIS':
            modulos.push('Materiais');
            break;
        case 'ORCAMENTO':
            modulos.push('Orcamento');
            break;
        case 'SPED':
            modulos.push('SPED');
            break;
        case 'TRIBUTOS':
            modulos.push('Tributos');
            break;
        case 'FINANCEIRODOIS':
            modulos.push('FinanceiroDois');
            break;
        default:
            modulos.push('Tributos');
            modulos.push('SPED');
            modulos.push('Orcamento');
            modulos.push('Materiais');
            modulos.push('Genericos');
            modulos.push('Financeiro');
            modulos.push('FinanceiroDois');
            modulos.push('Contabil');
            modulos.push('Comissoes');
            modulos.push('Comercial');
            modulos.push('Ativo');
            break;
    }

    //Percorrer módulo a módulo para preenche-lo conforme retorno do servidor
    var modAtual = null;
    var hndEmpresa = $('#Handle').val()
    modulos.forEach(function (item, indice) {
        $.ajax({
            type: "GET",
            url: (Benner.Page.getApplicationPath() + 'api/Erp360/' + hndEmpresa + '/ParametrosPorEmpresa?modulo=' + item),
            data: null,
            contentType: "json",
            success: function (data, status) {
                if (data != null) {
                    //attrs = (retorno do servidor é um objeto contendo o nome do campo e seu respectivo valor)
                    //Varremos os atributos e valores adicionando aos campos conforme o seu TIPO.
                    var attrs = JSON.parse(data)
                    if (attrs != null) {
                        for (var propertyName in attrs.Fields) {
                            var $cmpRefParametro = $('#Parametros' + item + ' #' + propertyName.toUpperCase());                               

                            //Se o campo referente ao ID não for localizado, ele é do tipo ID CONJUNTO.
                            if ($cmpRefParametro.length < 1) {
                                var ctrPesquisa = 'a[id*="' + propertyName.toUpperCase() + '"]a[id*=","]';
                                $cmpRefParametro = $(ctrPesquisa);

                                //Existe o módulo financeiro II, que não é encontrado e devemos procura-lo no financeiro um.
                                if ($cmpRefParametro.length < 1) {
                                    $cmpRefParametro = $('#ParametrosFinanceiro #' + propertyName.toUpperCase());
                                }
                            }

                            PreencherCamposParametrosPorEntidade(attrs.Fields, $cmpRefParametro, propertyName);                            
                        }
                    }
                }
            },
            error: function (data) {
                LimparCamposCadastroEmpresa();
                $('#Handle').val('');
            }
        });
    });    
}

function PreencherCamposParametrosPorEntidade(_fields, _cmpRef, _prpName) {
    try {

        //Os atributos podem ser string, ou listas contendo outros atributos(nos casos de DROPDOWNLIST)
        switch (typeof (_fields[_prpName])) {
            case 'string':
                _cmpRef.editable('setValue', _fields[_prpName].toString());
                break;
            case 'number':
                _cmpRef.editable('setValue', _fields[_prpName].toString());
                break;
            case 'boolean':
                var vlAtual = _cmpRef.attr('data-value');
                if (_fields[_prpName]) {                    
                    if (vlAtual !== undefined)
                        _prpName += (',' + vlAtual);

                    _cmpRef.attr('data-value', _prpName);
                    _cmpRef.editable('setValue', _prpName);
                }
                break;
            default:
                //Para os tipos lista, percorremos para obter o valor e adicionar o que estiver selecionado.
                for (var propFilha in _fields[_prpName]) {
                    var propValue = _fields[_prpName][propFilha];


                    if (propFilha == 'Value') {
                        _cmpRef.val(propValue);
                        _cmpRef.editable('setValue', propValue);
                    }
                    else if (propFilha == 'Text') {
                        _cmpRef.text(propValue);
                    }
                    else if (propFilha == 'SerializableEntity') {
                        for (var propSelect in eval(propValue).Fields) {

                            var propSelValue = eval(propValue).Fields[propSelect];

                            if (propSelect == 'HANDLE') {
                                _cmpRef.val(propSelValue);
                                _cmpRef.editable('setValue', propSelValue);
                                _cmpRef.attr('data-value', propSelValue);
                            }
                            else if (propSelect == 'NOME') {
                                _cmpRef.text(propSelValue);
                            }
                        }
                    }
                }
                break;
        }
    } catch (e) {
        console.log(e, _fields[_prpName], _prpName, _prpConjugado);
    }    
}

