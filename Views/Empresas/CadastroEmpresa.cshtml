            contentType: "json",
            success: function (data, status) {
                if (data != null) {
                    var empresa = eval(data)[0].Fields;
                    PreencherCamposCadastroEmpresa(empresa);
                    $.fn.editable.defaults.mode = 'inline';
                    $('.table-striped a').each(function (i, cmp) {

                        var $campoAtual = $(cmp);

                        switch ($campoAtual.attr('data-type')) {
                            case 'select2':
                                setTimeout(function () {
                                    Select2($campoAtual);
