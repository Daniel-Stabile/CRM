function BuscarDadosPorCNPJ(campo) {

    cnpj = $(campo).inputmask('unmaskedvalue');
    if (cnpj != '') {
        $.ajax({
            type: "GET",
            url: Benner.Page.getApplicationPath() + 'api/Erp360/' + cnpj + '/PorCNPJ',
