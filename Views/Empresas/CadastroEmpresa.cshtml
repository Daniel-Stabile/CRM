
@{
    ViewBag.Title = "Cadastro de empresa";
}

<link href="~/Content/css/bootstrap-editable.css" rel="stylesheet" />
<style type="text/css">
    .table-striped tbody tr td:first-child {
        width: 30%;
        padding-left: 1.5%;
    }

    .extended tbody tr td:first-child {
        width: 40%;
    }

    .table a {
        color: #4B77BE !important;
        text-decoration: none;
        border-bottom: 1px #4B77BE dotted;
        font-size: 12px;
    }

    .table td {
        font-size: 13px;
    }

    .ver-inline-menu li.active a, .ver-inline-menu li.active i {
        background-color: #4B77BE !important;
    }

    .ver-inline-menu li.active:after {
        border-left: 6px solid #4B77BE;
    }

    .ver-inline-menu li.active a {
        border-left: solid 2px #144594;
    }

    .centro {
        width: 20%;
    }

    .separador td {
        background-color: #f0f6fa;
        color: #557386;
        text-align: left;
        padding-left: 5px !important;
    }

    .hide {
        display: none;
    }
    .right {
        text-align:right;
    }

    ul.trash {
      list-style: none;
      position:absolute;
      bottom: 50%;
      left: 45%;
      display: none;
    }
    ul.trash li {
      display: inline-block;
      margin: 10px;
    }
    ul.trash li a {
      font-size: 1em;
      color: #fff;
      display: table-cell;
      vertical-align: middle;
      text-align: center;
      text-decoration: none;
      height: 75px;
      width: 75px;
      border-radius: 50% !important;
      background-color: #4B77BE;
      line-height: 100%;
      padding-top: 10px;
    }

    #btnSalvar {
        margin-bottom: 2%;
        border-left-color: #4B77BE
    }

    .dataurl {
        cursor:move
    }

</style>

<div class="row profile-account green-dark" id="pgCadastro">
    <div class="col-md-2">
        <ul class="ver-inline-menu tabbable margin-bottom-10">
            <li class="active"><a data-toggle="tab" href="#CadastroEmpresa" aria-expanded="true"><i class="fa fa-user"></i> Empresa </a><span class="after"> </span></li>
            <li><a data-toggle="tab" href="#ParametrosAtivo" id="tabAtivo" aria-expanded="true"><i class="fa fa-picture-o"></i> Ativo </a></li>
            <li><a data-toggle="tab" href="#ParametrosComercial" aria-expanded="true"><i class="fa fa-cubes"></i> Comercial </a></li>
            <li><a data-toggle="tab" href="#ParametrosComissoes" aria-expanded="true"><i class="fa fa-bars"></i> Comissões </a></li>
            <li><a data-toggle="tab" href="#ParametrosContabil" aria-expanded="true"><i class="fa fa-calculator"></i> Contábil </a></li>
            <li><a data-toggle="tab" href="#ParametrosFinanceiro" aria-expanded="true"><i class="fa fa-money"></i> Financeiro </a></li>
            <li><a data-toggle="tab" href="#ParametrosGenericos" aria-expanded="true"><i class="fa fa-database"></i> Genéricos </a></li>
            <li><a data-toggle="tab" href="#ParametrosMateriais" aria-expanded="true"><i class="fa fa-paint-brush"></i> Materiais </a></li>
            <li><a data-toggle="tab" href="#ParametrosOrcamento" aria-expanded="true"><i class="fa fa-usd"></i> Orçamento </a></li>
            <li><a data-toggle="tab" href="#ParametrosSPED" aria-expanded="true"><i class="fa fa-file-text-o"></i> SPED </a></li>
            <li><a data-toggle="tab" href="#ParametrosTributos" aria-expanded="true"><i class="fa fa-university"></i> Tributos </a></li>
        </ul>        
    </div>
    <div class="col-md-10">
        <div class="tab-content">
            @*      CADASTRO DA EMPRESA     *@
            <div class="tab-pane active" id="CadastroEmpresa">
                <div class="row">
                    <div class="col-md-12">
                        <div class="row">
                            <div class="col-md-12">
                                
                                <a href="javascript:;" class="btn default blue-stripe" id="btnSalvar" onclick="SalvarCadastroEmpresa();"><i class="fa fa-spinner fa-spin hide"></i> Salvar e continuar </a>
                            </div>
                        </div>
                        <div class="alert alert-danger hide" id="divAlerta">
                            <strong>Atenção!</strong>
                            <ul id="ulErros"></ul>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <h4 class="form-section"><i class="fa fa-user"></i> Identificação</h4>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-9">
                                <div class="row">
                                    <div class="col-md-4 col-sm-5 col-lg-4">
                                        <div class="form-group">
                                            <label for="CNPJ">CNPJ<em class="required" aria-required="true">*</em></label>
                                            <input type="text" required id="CNPJ" name="CNPJ" class="form-control" onchange="BuscarDadosPorCNPJ(this)" maxlength="18">
                                        </div>
                                    </div>
                                    <div class="col-md-8 col-sm-7 col-lg-8">
                                        <div class="form-group">
                                            <label for="NOME">Razão social<em class="required" aria-required="true">*</em></label>
                                            <input type="text" required id="NOME" name="NOME" class="form-control" maxlength="100">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="NOMEFANTASIA">Nome fantasia<em class="required" aria-required="true">*</em></label>
                                            <input name="NOMEFANTASIA" required type="text" value="" maxlength="120" id="NOMEFANTASIA" class="form-control">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="col-md-12">
                                    <div class="dropzone dropzone-file-area dz-clickable" style="border-color:#36c6d3;cursor:move" draggable="true">
                                        <h3 class="sbold">Seu logotipo!</h3>
                                        <p id="descricaoLogo">Arraste-o até aqui.</p>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <h4 class="form-section" style="margin-top: 3%"><i class="fa fa-map-marker"></i> Endereço</h4>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-2 col-sm-3">
                                <div class="form-group">
                                    <label for="CEP">CEP<em class="required" aria-required="true">*</em></label>
                                    <input type="text" required id="CEP" class="form-control" maxlength="9">
                                </div>
                            </div>

                            <div class="col-md-2 col-sm-3">
                                <div class="form-group">
                                    <label for="TIPOLOGRADOURO">Tipo logr.<em class="required" aria-required="true">*</em></label>
                                    <select name="TIPOLOGRADOURO" required id="TIPOLOGRADOURO" class="form-control benner-search select2-hidden-accessible">]
                                        <option value=""></option>
                                        <option value="1">Aeroporto</option>
                                        <option value="2">Alameda</option>
                                        <option value="3">Área</option>
                                        <option value="4">Avenida</option>
                                        <option value="5">Campo</option>
                                        <option value="6">Chácara</option>
                                        <option value="7">Colônia</option>
                                        <option value="8">Condomínio</option>
                                        <option value="9">Conjunto</option>
                                        <option value="10">Distrito</option>
                                        <option value="11">Esplanada</option>
                                        <option value="12">Estação</option>
                                        <option value="13">Estrada</option>
                                        <option value="14">Favela</option>
                                        <option value="15">Fazenda</option>
                                        <option value="16">Feira</option>
                                        <option value="17">Jardim</option>
                                        <option value="18">Ladeira</option>
                                        <option value="19">Lago</option>
                                        <option value="20">Lagoa</option>
                                        <option value="21">Largo</option>
                                        <option value="22">Loteamento</option>
                                        <option value="23">Morro</option>
                                        <option value="24">Outros</option>
                                        <option value="25">Parque</option>
                                        <option value="26">Passarela</option>
                                        <option value="27">Pátio</option>
                                        <option value="28">Praça</option>
                                        <option value="29">Quadra</option>
                                        <option value="30">Recanto</option>
                                        <option value="31">Residencial</option>
                                        <option value="32">Rodovia</option>
                                        <option value="33">Rua</option>
                                        <option value="34">Setor</option>
                                        <option value="35">Sítio</option>
                                        <option value="36">Travessa</option>
                                        <option value="37">Trecho</option>
                                        <option value="38">Trevo</option>
                                        <option value="39">Vale</option>
                                        <option value="40">Vereda</option>
                                        <option value="41">Via</option>
                                        <option value="42">Viaduto</option>
                                        <option value="43">Viela</option>
                                        <option value="44">Vila</option>
                                        <option value="45">Praia</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-6 col-sm-3">
                                <div class="form-group">
                                    <label for="LOGRADOURO">Logradouro<em class="required" aria-required="true">*</em></label>
                                    <input type="text" required id="LOGRADOURO" class="form-control">
                                </div>
                            </div>

                            <div class="col-md-2 col-sm-3">
                                <div class="form-group">
                                    <label for="NUMERO">N°</label>
                                    <input type="text" id="NUMERO" class="form-control">
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-2 col-sm-3">
                                <div class="form-group">
                                    <label for="Pais">País<em class="required" aria-required="true">*</em></label>
                                    <select name="Pais" required id="Pais" class="benner-search select2-hidden-accessible" data-targetentity="PAISES" data-resultfields="NOME" data-label="Pais" data-type="association" data-field="PAIS" data-sourceentity="EMPRESAS" data-lookupviewname="PAISES.LOOKUP" data-closeonselect="true" data-placeholder="Selecione" data-systeminstancename="CORPORATIVO" style="display: inline-block;" tabindex="-1" aria-hidden="true"></select>
                                </div>
                            </div>

                            <div class="col-md-2 col-sm-3">
                                <div class="form-group">
                                    <label for="ESTADO">Estado<em class="required" aria-required="true">*</em></label>
                                    <select name="ESTADO" required id="ESTADO" class="benner-search select2-hidden-accessible" data-targetentity="ESTADOS" data-resultfields="NOME" data-label="Estado" data-type="association" data-field="ESTADO" data-dependlist="$PAIS$" data-sourceentity="EMPRESAS" data-lookupviewname="ESTADOS.LOOKUP" data-placeholder="Selecione" data-closeonselect="true" data-systeminstancename="CORPORATIVO" data-url="" style="display: inline-block;" tabindex="-1" aria-hidden="true"></select>
                                </div>
                            </div>

                            <div class="col-md-5 col-sm-3">
                                <div class="form-group">
                                    <label for="MUNICIPIO">Município<em class="required" aria-required="true">*</em></label>
                                    <select name="MUNICIPIO" required id="MUNICIPIO" class="benner-search select2-hidden-accessible" data-targetentity="MUNICIPIOS" data-resultfields="NOME" data-label="Município" data-type="association" data-field="MUNICIPIO" data-dependlist="$ESTADO$" data-sourceentity="EMPRESAS" data-lookupviewname="MUNICIPIOS.LOOKUP" data-placeholder="Selecione" data-closeonselect="true" data-systeminstancename="CORPORATIVO" data-url="" style="display: inline-block;" tabindex="-1" aria-hidden="true"></select>
                                </div>
                            </div>

                            <div class="col-md-3 col-sm-3">
                                <div class="form-group">
                                    <label for="BAIRRO">Bairro</label>
                                    <input type="text" id="BAIRRO" class="form-control">
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <h4 class="form-section" style="margin-top: 3%"><i class="fa fa-envelope"></i> Contato</h4>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="CONTATO">Nome da pessoa</label>
                                    <input type="text" id="CONTATO" name="CONTATO" class="form-control">
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label for="TELEFONE">Telefone<i>(Fixo)</i></label>
                                    <input type="text" id="TELEFONE" name="TELEFONE" class="form-control">
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label for="TELEFONE2">Telefone<i>(Celular)</i></label>
                                    <input type="text" id="TELEFONE2" name="TELEFONE2" class="form-control">
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="CORREIOELETRONICO">Email</label>
                                    <input type="email" id="CORREIOELETRONICO" name="CORREIOELETRONICO" class="form-control">
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="CAIXAPOSTAL">Caixa postal</label>
                                    <input type="text" id="CAIXAPOSTAL" name="CAIXAPOSTAL" class="form-control">
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="FAX">FAX</label>
                                    <input type="text" id="FAX" name="FAX" class="form-control">
                                </div>
                            </div>
                            <input type="hidden" id="Handle" name="Handle" value="" />
                            @Html.AntiForgeryToken()
                        </div>
                    </div>
                </div>
            </div>

            @*      PARÂMETROS ATIVO     *@
            <div id="ParametrosAtivo" class="tab-pane">

                <div class="row">
                    <div class="col-md-12">


                        <div class="panel-group accordion" id="AccordionAtivo">
                            @*      GERAL     *@
                            <div class="panel panel-default">

                                <div class="panel-heading">
                                    <h4 class="panel-title">
                                        <a class="accordion-toggle accordion-toggle-styled" data-toggle="collapse" data-parent="#AccordionAtivo" href="#AtivoGeral">Gerais</a>
                                    </h4>
                                </div>

                                <div id="AtivoGeral" class="panel-collapse in">

                                    <div class="panel-body">
                                        <table class="table table-bordered table-striped extended">
                                            <tbody>
                                                <tr>
                                                    <td>Índice oficial</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" data-table="GN_MOEDAS" id="INDICECORRECAO" data-type="select2" class="editable editable-click"></a></td>
                                                    <td class="centro">Índice alternativo</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" data-table="GN_MOEDAS" id="INDICEALTERNATIVO" data-type="select2" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Contador req. de movimentação</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="CONTADORITEMMOVIMENTACAOREQ" data-type="select2" data-table="Z_CONTADORES" class="editable editable-click"></a></td>
                                                    <td class="centro">Data implantação</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="DATAIMPLANTACAO" data-type="date" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Forma depreciação</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="DEPRECIACAO" data-type="select" data-source="[{value:1,text:'Total'}, {value:2, text:'Pró-rata'}]" class="editable editable-click"></a></td>
                                                    <td class="centro">Deprecia mês aquisição</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="DEPRECIAMESAQUISICAO" data-type="checklist" data-options="S|N" data-source="[{value:'DEPRECIAMESAQUISICAO',text:'No mês da aquisição'}]" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Plaqueta</td>
                                                    <td colspan="3"><a href="javascript:;" data-action="ParametrosAtivo" id="TABPLAQUETA" data-type="select" data-source="[{value:1,text:'Digitar'}, {value:2, text:'Não digitar'}]" data-cb="{id: 'lPrefixo', value:'1'}" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr id="lPrefixo">
                                                    <td>Prefixo</td>
                                                    <td colspan="3"><a href="javascript:;" data-action="ParametrosAtivo" id="PREFIXOLOCALIZACAO,UTILIZASUFIXO,CORRIGE" data-type="checklist" data-options="S|N" data-source="[{value:'PREFIXOLOCALIZACAO',text:'Utilizar prefixo'}, {value:'UTILIZASUFIXO', text:'Utiliza sufixo'},{value:'CORRIGE',text:'Corrige'}]" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Gerais</td>
                                                    <td colspan="3"><a href="javascript:;" data-action="ParametrosAtivo" id="DEPRECIA,EXIGIRCENTROCUSTO,EFETIVATRANSFERENCIA" data-options="S|N" data-type="checklist" data-source="[{value:'DEPRECIA', text:'Deprecia'},{value:'EXIGIRCENTROCUSTO',text:'Exigir centro de custo'}, {value:'EFETIVATRANSFERENCIA', text:'Efetiva transferência'}]" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Rel. Padrão do cálculo do ativo</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="RELATORIOPADRAOCALCULOATIVO" data-table="R_RELATORIOS" data-type="select2" class="editable editable-click"></a></td>
                                                    <td class="centro">Modelo coletor</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="COLETORMODELO" data-type="select" data-source="[{value:1,text:'Coletor.exe'}, {value:2, text:'IPE Palm'},{value:3,text:'Manual'}, {value:4, text:'IPE Palm v2'}]" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Origem informação<i><small>SPED</small></i></td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="ORIGEMINFORMACAO" data-table="ED_ORIGENSINFOSLIVROS" data-type="select2" class="editable editable-click"></a></td>
                                                    <td class="centro">Motivo venda</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="MOTIVOVENDA" data-table="AT_MOTIVOSBAIXA" data-type="select2" class="editable editable-click"></a></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>

                            @*      CONTÁBIL     *@
                            <div class="panel panel-default">

                                <div class="panel-heading">
                                    <h4 class="panel-title">
                                        <a class="accordion-toggle accordion-toggle-styled collapsed" data-toggle="collapse" data-parent="#AccordionAtivo" href="#AtivoContabil">Contabilidade</a>
                                    </h4>
                                </div>

                                <div id="AtivoContabil" class="panel-collapse collapse">

                                    <div class="panel-body">
                                        <table class="table table-bordered table-striped">
                                            <tbody>
                                                <tr>
                                                    <td>Documento contábil</td>
                                                    <td colspan="3"><a href="javascript:;" data-action="ParametrosAtivo" id="DOCUMENTOCONTABIL" data-type="select" data-source="[{value:1,text:'Númeração automática'},{value:2,text:'Digitar'}]" data-cb="{id: 'lContadorAtivo', value:'1'}" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Prefixo</td>
                                                    <td colspan="3"><a href="javascript:;" data-action="ParametrosAtivo" id="PREFIXOATF" data-type="text" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Liberações de lotes ATF</td>
                                                    <td colspan="3"><a href="javascript:;" data-action="ParametrosAtivo" id="LIBERACAODELOTESATF" data-type="select" data-source="[{value: 1, text: 'Online'},{value: 2, text: 'Batch'}]" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Operação contábil tras. Filial</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="OPERACAOCONTABILTRAFILIAL" data-type="select2" data-table="GN_OPERACOESCONTABEIS" class="editable editable-click"></a></td>
                                                    <td class="centro">Contabilizar recup. De impostos</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="CONTABILIZARRECUPIMPOSTOS" data-type="select" data-source="[{value: 'C', text: 'Com centro de custo e projeto'},{value: 'S', text: 'Sem centro de custo e projeto'}]" class="editable editable-click"></a></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>

                            </div>

                            @*      RECUPERAÇÃO PIS/COFINS     *@
                            <div class="panel panel-default">

                                <div class="panel-heading">
                                    <h4 class="panel-title">
                                        <a class="accordion-toggle accordion-toggle-styled collapsed" data-toggle="collapse" data-parent="#AccordionAtivo" href="#AtivoPISCOFINS">Recuperação PIS/COFINS</a>
                                    </h4>
                                </div>

                                <div id="AtivoPISCOFINS" class="panel-collapse collapse">

                                    <div class="panel-body">
                                        <table class="table table-bordered table-striped">
                                            <tbody>
                                                <tr>
                                                    <td>Dt. Início recuperação</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="DATAINICIORECUPERACAO" data-type="date" class="editable editable-click date"></a></td>
                                                    <td class="centro">Imposto PIS</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="IMPOSTOPIS" data-table="TR_IMPOSTOS" data-type="select2" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Imposto COFINS</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="IMPOSTOCOFINS" data-table="TR_IMPOSTOS" data-type="select2" class="editable editable-click"></a></td>
                                                    <td class="centro">Op. Contábil PIS/CONFINS</td>
                                                    <td><a href="javascript:;" data-action="ParametrosAtivo" id="NOVAOPERACAOCONTABILPISCOFINS" data-table="GN_OPERACOESCONTABEIS" data-type="select2" class="editable editable-click"></a></td>
                                                </tr>
                                                <tr>
                                                    <td>Origem da ident. Dos bens</td>
