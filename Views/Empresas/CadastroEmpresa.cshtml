function Checklist(cmp) {

    $(cmp).editable({
        type: 'checklist',
        value: $(cmp).attr('data-value'),
        source: $(cmp).attr('data-source'),
        emptytext: 'Não informado',
        pk: $('#Handle').val(),
        url: (Benner.Page.getApplicationPath() + 'Empresas/' + $(cmp).attr('data-action')),
        success: function (response, newValue) {
            ExecutarRetorno($(cmp), newValue);
            $(cmp).attr('data-value', newValue.join());
            $(cmp).editable('setValue', newValue.join());
        },
        error: function (response) {
            ExecutarRetorno($(cmp));
            return response.responseJSON;
        },
        params: function (params) {
            params.pk = $(this).attr('data-pk');
            params.opt = $(this).attr('data-options');
            return params;
        },
        savenochange: true,
    });
}

