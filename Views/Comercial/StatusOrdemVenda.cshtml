

@if(Model["STATUS"].GetInt32() != null){
    if(Model["STATUS"].GetInt32() == 1){
        <label class="label label-info">
            Cadastrada
        </label>   
    }
    if(Model["STATUS"].GetInt32() == 2){
        <label class="label label-primary">
            Confirmada
        </label>   
    }
    if(Model["STATUS"].GetInt32() == 3){
        <label class="label label-info">
            Em faturamento
        </label>   
    }
    if(Model["STATUS"].GetInt32() == 4){
        <label class="label label-default">
            Encerrada
        </label>   
    }
    if(Model["STATUS"].GetInt32() == 5){
        <label class="label label-danger">
            Cancelada
        </label>   
    }
    if(Model["STATUS"].GetInt32() == 6){
        <label class="label label-success">
            Faturada
        </label>   
    }
    if(Model["STATUS"].GetInt32() == 7){
        <label class="label label-warning">
            Pendente
        </label>   
    }
} else {
    <label class="label label-danger">
        Nulo
    </label>   
}
