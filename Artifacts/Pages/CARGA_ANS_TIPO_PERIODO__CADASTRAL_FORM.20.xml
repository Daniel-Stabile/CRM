      <Id>FDDF75ABC9FC4D5FB10D66E0BF04CDE4</Id>
      <ResourceType>JavaScript</ResourceType>
      <Content><![CDATA[$(function(){

var cmp = $('input[type="radio"][value="'+getParameterByName('tipo')+'"]');
if (cmp.length > 0) {
 cmp.trigger('click');
 setTimeout(function() {$('[data-field="TIPO"]').find(':input[type="radio"]').attr('disabled', true)}, 1000);
}

})

function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }
]]></Content>
