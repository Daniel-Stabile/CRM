    <UserInputRequired>false</UserInputRequired>
    <Required>false</Required>
    <IsManuallyDefined>false</IsManuallyDefined>
  </FilterCondition>
</ArrayOfFilterCondition>
]]></ConditionsContent>
      <VirtualTableFormView>CM_FILTROBASICOFATURAMENTO.CUSTOM.FORM</VirtualTableFormView>
      <Visible>true</Visible>
    </FilterItemPortable>
  </Filters>
</FilterPortable>