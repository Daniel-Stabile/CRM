<?xml version="1.0"?>
<ScriptPortable xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ViewName>FN_DOCUMENTOS.FATURAMENTO.PREVIEWNOTA.FORM</ViewName>
  <EntityName>FN_DOCUMENTOS</EntityName>
  <Level>Benner</Level>
  <Scripts>
    <ScriptItemPortable>
      <FunctionName>Load</FunctionName>
      <LanguageName>IronPython</LanguageName>
      <SourceCode>def Load(widget):
    registro = widget.Entity
    if registro == None: return
    status = int(registro.Fields["STATUS"].Value)
    widget.View.Commands["CMD_EXCLUIR"].Visible = widget.View.Commands["CMD_EMITIR"].Visible = (status == 1) or (status == 4)
    widget.View.Commands["CMD_BLOQUETO"].Visible = (status == 2)
</SourceCode>
    </ScriptItemPortable>
    <ScriptItemPortable>
      <FunctionName>OnCMD_BLOQUETOExecute</FunctionName>
      <LanguageName>IronPython</LanguageName>
      <SourceCode>def OnCMD_BLOQUETOExecute(command, entity):
    command.OutputDefinition.Parameters["DOCUMENTO"] = entity.Fields["DOCUMENTOORIGEM"].Handle
</SourceCode>
    </ScriptItemPortable>
  </Scripts>
</ScriptPortable>