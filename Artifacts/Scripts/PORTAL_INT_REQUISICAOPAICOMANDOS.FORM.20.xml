<?xml version="1.0"?>
<ScriptPortable xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ViewName>PORTAL_INT_REQUISICAOPAICOMANDOS.FORM</ViewName>
  <EntityName>CP_REQUISICOESPAI</EntityName>
  <Level>Benner</Level>
  <Scripts>
    <ScriptItemPortable>
      <FunctionName>OnVisualizarAssinaturasExecute</FunctionName>
      <LanguageName>IronPython</LanguageName>
      <SourceCode>def OnVisualizarAssinaturasExecute(command, entity):
    command.OutputDefinition.Parameters.Add("HANDLEREQUISICAO", entity["HANDLE"].Value)
</SourceCode>
    </ScriptItemPortable>
    <ScriptItemPortable>
      <FunctionName>Load</FunctionName>
      <LanguageName>IronPython</LanguageName>
      <SourceCode>def Load(widget):

    if widget.Entity.Fields["LIBERADA"] == True : 
        widget.Commands.Remove("CUSTOM_FINALIZAR")
        widget.Commands.Remove("CUSTOM_NOVO")
</SourceCode>
    </ScriptItemPortable>
  </Scripts>
</ScriptPortable>