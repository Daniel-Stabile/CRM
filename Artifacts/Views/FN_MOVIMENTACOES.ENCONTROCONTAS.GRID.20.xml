        <visibility>Visible</visibility>
        <Value>@{
    string abrangencia = @Model["PATH_ABRANGENCIA"].GetString();
    string entradaSaida = @Model["PATH_ENTRADASAIDA"].GetString();
    string tipoMovimento = @Model["PATH_TIPOMOVIMENTO"].GetString();
    
    bool ehCre = tipoMovimento == "Normal" &amp;&amp;
    (entradaSaida == "Saída" || entradaSaida == "Saída bloqueada") &amp;&amp;
    (abrangencia == "Não" || abrangencia == "Todos");
    
    string documento = "";
    string classeCSS = "";

    documento = ehCre ? "CRE" : "CPA";
    classeCSS = ehCre ? "label label-sm label-success" : "label label-sm label-warning";
}
&lt;div class="@classeCSS"&gt;
    @documento
&lt;/div&gt;</Value>
