        <Expression> SELECT  COALESCE(SUM(E.VALOR-E.VALORUTILIZADO-E.VALORENCERRADO), 0)           FROM CT_PEDIDOSEMPENHOS E                INNER JOIN OP_ACOESPROJETOS P ON P.HANDLE = A.ACOESPROJETOS                LEFT JOIN CT_CONTASGERENCIAIS CG ON CG.HANDLE = E.CONTA                LEFT JOIN CT_VERSOESORCAMENTOS V ON V.HANDLE = E.VERSAO          WHERE E.ACOESPROJETOS = A.ACOESPROJETOS                AND ( CG.ORCAPROJETO = 'N' OR E.PROJETO = P.PROJETOGERENCIAL )                AND ( CG.ORCACENTROCUSTO = 'N' OR E.CENTROCUSTO = P.CENTROCUSTO )                AND P.CONTA = E.CONTA                AND ( @DATAPARTE(ANO, V.MESINICIAL)= @DATAPARTE(ANO, A.ANO )) </Expression>
        <FieldType>Currency</FieldType>
