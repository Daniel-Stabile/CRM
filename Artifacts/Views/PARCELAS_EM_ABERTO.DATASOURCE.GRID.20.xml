      <GridFieldDefinition xsi:type="GridFieldCustomDefinition" title="Valor parcela" key="CUSTOM_VALORPARCELA">
        <visible>false</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required xsi:nil="true" />
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>false</searchable>
        <visibility>ToScript</visibility>
        <Value>@{
    var valorCampo = Convert.ToDecimal(0);
    
    if(Model["ABATIMENTOS"].GetDBValue() &gt; 0){
        valorCampo += Model["ABATIMENTOS"].GetDBValue();
    }
    if(DateTime.Now.Month &lt;= Model["DATALIMITEDESCONTOCONDICIONAL"].GetDateTime().Month &amp;&amp; DateTime.Now.Day &lt;= Model["DATALIMITEDESCONTOCONDICIONAL"].GetDateTime().Day)
    {
        valorCampo += Model["DESCONTOCONDICIONAL"].GetDBValue();
    }

    decimal valor = Model["VALOR"].GetDBValue() - Model["VALORESBAIXADOS"].GetDBValue();
    
    if(Model["ACRESCIMOS"].GetDBValue() &gt; 0){
        valor += Model["ACRESCIMOS"].GetDBValue();
    }
    if(valorCampo &gt; 0){
        valor -= valorCampo.ToDecimal();
    }
}

@valor.ToString("N2")</Value>
