      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Total Reservar" key="TOTAL_RESERVAR">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression> SELECT   COALESCE(A.VALOR,0) - COALESCE(SUM(E.VALOR), 0)           FROM CT_PEDIDOSEMPENHOS E                INNER JOIN OP_ACOESPROJETOS P ON P.HANDLE = A.ACOESPROJETOS                LEFT JOIN CT_CONTASGERENCIAIS CG ON CG.HANDLE = E.CONTA                LEFT JOIN CT_VERSOESORCAMENTOS V ON V.HANDLE = E.VERSAO          WHERE E.ACOESPROJETOS = A.ACOESPROJETOS                AND ( CG.ORCAPROJETO = 'N' OR E.PROJETO = P.PROJETOGERENCIAL )                AND ( CG.ORCACENTROCUSTO = 'N' OR E.CENTROCUSTO = P.CENTROCUSTO )                AND P.CONTA = E.CONTA                AND ( @DATAPARTE(ANO, V.MESINICIAL)= @DATAPARTE(ANO, A.ANO )) </Expression>
        <FieldType>Currency</FieldType>
        <SystemInstanceName>TERRACAP</SystemInstanceName>
      </GridFieldDefinition>
      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Total Ped. Empenho" key="TOTAL_PEDIDO_EMPENHO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>   SELECT COALESCE(SUM(E.VALOR), 0)           FROM CT_PEDIDOSEMPENHOS E                INNER JOIN OP_ACOESPROJETOS P ON P.HANDLE = A.ACOESPROJETOS                LEFT JOIN CT_CONTASGERENCIAIS CG ON CG.HANDLE = E.CONTA                LEFT JOIN CT_VERSOESORCAMENTOS V ON V.HANDLE = E.VERSAO          WHERE E.ACOESPROJETOS = A.ACOESPROJETOS                AND ( CG.ORCAPROJETO = 'N' OR E.PROJETO = P.PROJETOGERENCIAL )                AND ( CG.ORCACENTROCUSTO = 'N' OR E.CENTROCUSTO = P.CENTROCUSTO )                AND P.CONTA = E.CONTA                AND ( @DATAPARTE(ANO, V.MESINICIAL)= @DATAPARTE(ANO, A.ANO )) AND E.STATUS IN (2,6,7)</Expression>
        <FieldType>Currency</FieldType>
        <SystemInstanceName>TERRACAP</SystemInstanceName>
      </GridFieldDefinition>
      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Total Empenho" key="TOTAL_EMPENHO">
        <visible>true</visible>
