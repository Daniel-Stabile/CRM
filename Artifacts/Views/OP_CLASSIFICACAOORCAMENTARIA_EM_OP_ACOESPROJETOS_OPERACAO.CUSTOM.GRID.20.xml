        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>SELECT COALESCE(SUM(E.VALOR), 0)   FROM CT_EMPENHOS E        INNER JOIN OP_ACOESPROJETOS P ON P.HANDLE = A.ACOESPROJETOS        LEFT JOIN CT_CONTASGERENCIAIS CG ON CG.HANDLE = E.CONTA        LEFT JOIN CT_VERSOESORCAMENTOS V ON V.HANDLE = E.VERSAO  WHERE E.ACOESPROJETOS = A.ACOESPROJETOS        AND        (            CG.ORCAPROJETO = 'N'            OR E.PROJETO = P.PROJETOGERENCIAL        )        AND        (            CG.ORCACENTROCUSTO = 'N'            OR E.CENTROCUSTO = P.CENTROCUSTO        )        AND P.CONTA = E.CONTA        AND ( @DATAPARTE(ANO, V.MESINICIAL)= @DATAPARTE(ANO, A.ANO ))        AND E.STATUS IN (1, 2) </Expression>
        <FieldType>Currency</FieldType>
        <SystemInstanceName>TERRACAP</SystemInstanceName>
      </GridFieldDefinition>
