        <SystemInstanceName>CRM</SystemInstanceName>
      </GridFieldDefinition>
      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Valor SaaS" key="VALORSAASSQL">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>SELECT CASE WHEN (SELECT 1 FROM K_CRM_PO_SAAS WHERE PRODUTO = @CAMPO(HANDLE)) IS NULL THEN 0 ELSE (SELECT CASE WHEN (SELECT 1 FROM K_CRM_MODALIDADES WHERE LUM = 'S' AND HANDLE IN (SELECT MODALIDADE FROM K_CRM_PESSOAOPORTUNIDADES WHERE HANDLE = @CAMPO(OPORTUNIDADE))) IS NOT NULL THEN ((SELECT VALOR FROM K_CRM_PO_SAAS WHERE PRODUTO = @CAMPO(HANDLE)) + @CAMPO(VALORLUM)) WHEN (SELECT 1 FROM K_CRM_MODALIDADES WHERE LOCACAO = 'S' AND HANDLE IN (SELECT MODALIDADE FROM K_CRM_PESSOAOPORTUNIDADES WHERE HANDLE = @CAMPO(OPORTUNIDADE))) IS NOT NULL THEN ((SELECT VALOR FROM K_CRM_PO_SAAS WHERE PRODUTO = @CAMPO(HANDLE)) + @CAMPO(VALORLOCACAO)) ELSE  (SELECT VALOR FROM K_CRM_PO_SAAS WHERE PRODUTO = @CAMPO(HANDLE)) END DC) END VALOR</Expression>
        <FieldType>Currency</FieldType>
        <SystemInstanceName>CRM</SystemInstanceName>
      </GridFieldDefinition>
