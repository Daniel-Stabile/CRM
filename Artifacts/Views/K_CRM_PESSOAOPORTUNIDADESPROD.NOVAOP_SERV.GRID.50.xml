      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Valor DC" key="VALORDC">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>SELECT CASE WHEN (SELECT 1 FROM K_CRM_PO_SAAS WHERE PRODUTO = @CAMPO(HANDLE)) IS NULL THEN 0 ELSE (SELECT VALOR FROM K_CRM_PO_SAAS WHERE PRODUTO = @CAMPO(HANDLE)) END VALOR</Expression>
