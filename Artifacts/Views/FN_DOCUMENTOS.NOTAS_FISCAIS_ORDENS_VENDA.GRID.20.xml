      <GridFieldDefinition xsi:type="GridFieldCustomDefinition" title="Situação" key="SITUACAO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required xsi:nil="true" />
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable xsi:nil="true" />
        <visibility>Visible</visibility>
        <Value>@{var statusNfe=Model.Fields["STATUSNFE"];
var nomeStatus = Model.Definition.FieldDefinitions["STATUSNFE"].ToUI(Model["STATUSNFE"].Value);
var statusMde=Model.BuscarStatusMDe();
var handleNFSE = Model["HANDLENFSE"].GetInt32();
var handleNFE = Model["HANDLENFE"].GetInt32();

if(handleNFE != 0)
{
    if(statusNfe == 1)
    {
    &lt;span class="badge badge-info badge-roundless col-md-12" style="background-color: #3598dc;"&gt;NFe Cadastrada&lt;/span&gt;
    }
    if((statusNfe == 2) || (statusNfe == 4))
    {
    &lt;span class="badge badge-danger badge-roundless col-md-12" style="background-color: #E7CA14;"&gt;NFe @nomeStatus&lt;/span&gt;
    }
    if(statusNfe == 3)
    {
    &lt;span class="badge badge-danger badge-roundless col-md-12" style="background-color: #E7CA14;"&gt;NFe Rejeitada&lt;/span&gt;
    }
    if(statusNfe == 5)
    {
    &lt;span class="badge badge-danger badge-roundless col-md-12"style="background-color: #E7CA14;"&gt;NFe Denegada&lt;/span&gt;
    }
    if(statusNfe == 7)
    {
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #E08283;"&gt;NFe Cancelada&lt;/span&gt;
    }
    if(statusNfe == 8)
    {
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #E08283;"&gt;NFe Inutilizada&lt;/span&gt;
    }
    if(statusNfe == 6)
    {
    &lt;span class="badge badge-success badge-roundless col-md-12" style="background-color: #1BBC9B;"&gt;NFe Autorizada&lt;/span&gt;
    }
    if((statusNfe == 9) || (statusNfe == 10))
    {
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #FCBA03;"&gt;NFe @nomeStatus&lt;/span&gt;
    }
}
else if(handleNFSE != 0)
{
    if(statusNfe == 1)
    {
    &lt;span class="badge badge-info badge-roundless col-md-12" style="background-color: #3598dc;"&gt;NFSe Cadastrada&lt;/span&gt;
    }
    if((statusNfe == 2) || (statusNfe == 4))
    {
    &lt;span class="badge badge-danger badge-roundless col-md-12" style="background-color: #E7CA14;"&gt;NFSe @nomeStatus&lt;/span&gt;
    }
    if(statusNfe == 3)
    {
    &lt;span class="badge badge-danger badge-roundless col-md-12" style="background-color: #E7CA14;"&gt;NFSe Rejeitada&lt;/span&gt;
    }
    if(statusNfe == 5)
    {
    &lt;span class="badge badge-danger badge-roundless col-md-12"style="background-color: #E7CA14;"&gt;NFSe Denegada&lt;/span&gt;
    }
    if(statusNfe == 7)
    {
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #E08283;"&gt;NFSe Cancelada&lt;/span&gt;
    }
    if(statusNfe == 8)
    {
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #E08283;"&gt;NFSe Inutilizada&lt;/span&gt;
    }
    if(statusNfe == 6)
    {
    &lt;span class="badge badge-success badge-roundless col-md-12" style="background-color: #1BBC9B;"&gt;NFSe Autorizada&lt;/span&gt;
    }
    if((statusNfe == 9) || (statusNfe == 10))
    {
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #FCBA03;"&gt;NFSe @nomeStatus&lt;/span&gt;
    }
}
else
{
    &lt;span class="badge badge-default badge-roundless col-md-12" style="background-color: #bfbfbf;"&gt;Sem NFe/NFSe&lt;/span&gt;
}
&lt;br/&gt;
if(statusMde == 7)
{
&lt;span class="badge badge-info badge-roundless col-md-12"style="background-color: #3598dc;"&gt;MDe Ciência da operação&lt;/span&gt;
}
if(statusMde == 8)
{
&lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #FCBA03;"&gt;MDe Confirmação da operação&lt;/span&gt;
}
if(statusMde == 9)
{
&lt;span class="badge badge-alert badge-roundless col-md-12"style="background-color: #E7CA14;"&gt;MDe Desconhecimento da operação&lt;/span&gt;
}
if(statusMde == 10)
{
&lt;span class="badge badge-danger badge-roundless col-md-12"style="background-color: #E08283;"&gt;MDe Operação não realizada&lt;/span&gt;
}
else if(statusMde == 0)
{
    &lt;span class="badge badge-default badge-roundless col-md-12" style="background-color: #bfbfbf;"&gt;MDe Sem manifestação&lt;/span&gt;
}
&lt;br/&gt;
if(Model.GerouFinanceiroFiscal())
{
    &lt;span class="badge badge-success badge-roundless col-md-12"style="background-color: #1BBC9B;"&gt;Gerou financ/fiscal&lt;/span&gt;
}
else if(Model.SemFinanceiroFiscal())
{
    &lt;span class="badge badge-default badge-roundless col-md-12"style="background-color: #bfbfbf;"&gt;Sem financ/fiscal&lt;/span&gt;
}
else
{
    &lt;span class="badge badge-danger badge-roundless col-md-12" style="background-color: #E08283;"&gt;Não gerou financ/fiscal&lt;/span&gt;
}
}</Value>
