          <Command title="Selecionar ordem de compra" key="CMD_SELECIONAR_OC">
            <visible>true</visible>
            <showModal>false</showModal>
            <modalSize xsi:nil="true" />
            <showInAllLevelsStructuredTable>false</showInAllLevelsStructuredTable>
            <runInEditMode>false</runInEditMode>
            <context>Table</context>
            <OutputDefinition xsi:type="GridLinkDefinition">
              <Url>~/aga/a/materiais/recebimentoFisico/selecionarOC.aspx</Url>
              <IsModalPage>false</IsModalPage>
              <IsModalNavigation>false</IsModalNavigation>
              <WhereClause>
                <Where>(A.SALDO *          (SELECT VALORUNITARIO           FROM CP_ORDENSCOMPRAITENS           WHERE HANDLE = A.ORDENSCOMPRAITENS) &gt; 0) 		  AND A.HANDLE NOT IN  (SELECT ORDEMCOMPRAENTREGA           FROM CP_RECEBIMENTOFISICOOCENTREGAS  		  WHERE RECEBIMENTOFISICO IN  		  (SELECT HANDLE FROM CP_RECEBIMENTOFISICO  		  WHERE RECEBIMENTOFISICOPAI = @RASTRO(CP_RECEBIMENTOFISICOPAI)))		     AND A.ORDENSCOMPRAITENS IN     (SELECT HANDLE      FROM CP_ORDENSCOMPRAITENS      WHERE ORDEMCOMPRA IN          (SELECT HANDLE           FROM CP_ORDENSCOMPRA           WHERE STATUS IN (2,                            3)             AND FORNECEDOR =               (SELECT FORNECEDOR                FROM CP_RECEBIMENTOFISICOPAI                WHERE HANDLE = @RASTRO(CP_RECEBIMENTOFISICOPAI))             AND EMPRESA =               (SELECT EMPRESA                FROM CP_RECEBIMENTOFISICOPAI                WHERE HANDLE = @RASTRO(CP_RECEBIMENTOFISICOPAI))             AND FILIAL =               (SELECT FILIAL                FROM CP_RECEBIMENTOFISICOPAI                WHERE HANDLE = @RASTRO(CP_RECEBIMENTOFISICOPAI))))</Where>
                <Parameters />
              </WhereClause>
              <SystemInstanceName>CORPORATIVO</SystemInstanceName>
              <TargetEntityDefinitionName>CP_ORDENSCOMPRAENTREGA</TargetEntityDefinitionName>
              <TargetSystemInstanceName>CORPORATIVO</TargetSystemInstanceName>
              <WhereClauseParameters />
