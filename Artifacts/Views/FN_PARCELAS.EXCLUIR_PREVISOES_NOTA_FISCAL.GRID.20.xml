        <Value>@{
    var historico = @Model["HISTORICO"].GetString();
    var truncar = !string.IsNullOrEmpty(historico) &amp;&amp; historico.Length &gt; 20;
    var historicoTruncado = truncar ? historico.Substring(0, 20) + "..." : string.Empty;
}
@if (truncar)
{
	&lt;a class="default popovers" style="text-decoration: none; cursor: default;" data-toggle="popover" data-html="true" data-placement="left" data-content="
		&lt;table class=&amp;quot;table&amp;quot; style= &amp;quot;font-size:11px  &amp;quot; &gt;
			&lt;tbody&gt;
				&lt;tr&gt;
					&lt;td&gt;@historico&lt;/td&gt;
				&lt;/tr&gt;
			&lt;/tbody&gt;
		&lt;/table&gt;" data-original-title="" data-trigger="hover"&gt;
		@historicoTruncado
	&lt;/a&gt;
} 
else 
{
    &lt;span&gt;@historico&lt;/span&gt;
}</Value>
        <ValueType>HTML</ValueType>
