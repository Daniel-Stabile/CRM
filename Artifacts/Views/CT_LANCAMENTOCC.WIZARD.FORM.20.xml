        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <ValueFromView>@if(Model.Fields["ROTULOPERCENTUALLANCADO"] != string.Empty){ 
    if (Convert.ToDecimal(Model.Fields["ROTULOPERCENTUALLANCADO"]) == 100){
        &lt;div class="dashboard-stat dashboard-stat-v2 wes-default-tile tilecolor command-name blue" data-class="note-info" style="padding: 10px;"&gt;
            &lt;h5 class="block" style="color: white;"&gt;@(Model.Fields["ROTULOPERCENTUALLANCADO"])%&lt;/h5&gt;
            &lt;h6 style="color: white;"&gt;Percentual de centro de custos&lt;/h6&gt;
        &lt;/div&gt;
    } else if(Convert.ToDecimal(@Model.Fields["ROTULOPERCENTUALLANCADO"]) &gt; 100){
        &lt;div class="dashboard-stat dashboard-stat-v2 wes-default-tile tilecolor command-name red-intense" data-class="note-info" style="padding: 10px;"&gt;
            &lt;h5 class="block" style="color: white;"&gt;@(Model.Fields["ROTULOPERCENTUALLANCADO"])%&lt;/h5&gt;
            &lt;h6 style="color: white;"&gt;Percentual de centro de custos&lt;/h6&gt;
        &lt;/div&gt;
    } else {
        &lt;div class="dashboard-stat dashboard-stat-v2 wes-default-tile tilecolor command-name yellow-crusta" data-class="note-info" style="padding: 10px;"&gt;
            &lt;h5 class="block" style="color: white;"&gt;@(Model.Fields["ROTULOPERCENTUALLANCADO"])%&lt;/h5&gt;
            &lt;h6 style="color: white;"&gt;Percentual de centro de custos&lt;/h6&gt;
        &lt;/div&gt;
    }
} else {
    &lt;div class="dashboard-stat dashboard-stat-v2 wes-default-tile tilecolor command-name blue" data-class="note-info" style="padding: 10px;"&gt;
        &lt;h5 class="block" style="color: white;"&gt;0%&lt;/h5&gt;
        &lt;h6 style="color: white;"&gt;Percentual de centro de custos&lt;/h6&gt;
    &lt;/div&gt;
}
</ValueFromView>
        <ValueType>HTML</ValueType>
