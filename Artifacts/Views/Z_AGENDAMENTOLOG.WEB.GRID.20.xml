      <GridFieldDefinition xsi:type="GridFieldCustomDefinition" title="Status" key="CUSTOM_STATUS">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required xsi:nil="true" />
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Value>@{
    string status  = Model["STATUS"].GetInt32().ToString();
    string statusDescription = Model["STATUS"].GetString();
    string statusClass = "default";
    switch (status)
    {
        case "1":
            statusClass = "info"; 
            break;
        case "2":
            statusClass = "success"; 
            statusDescription = "Sucesso";
            break;
        case "3":
            statusClass = "danger"; 
            statusDescription = "Com erro";
            break;
        case "4":
            statusClass = "default";
            break;
    }
}
&lt;div&gt;
    &lt;span class="label label-@statusClass"&gt;@statusDescription&lt;/span&gt;
&lt;/div&gt;</Value>
        <ValueType>HTML</ValueType>
      </GridFieldDefinition>
