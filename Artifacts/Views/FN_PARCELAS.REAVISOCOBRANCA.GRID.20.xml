        <Value>@{var DiasVencidos = (DateTime.Now.Date-Convert.ToDateTime(Model.Fields["DataVencimento"]).Date).TotalDays;}
@if (DiasVencidos &lt;= 0) 
{
    &lt;span class="badge badge-success badge-roundless"&gt;
    Sem atraso
    &lt;/span&gt;
}
@if (DiasVencidos &gt;=15 &amp;&amp; DiasVencidos &lt;= 30)
{
    &lt;span class="badge badge-warning badge-roundless"&gt;
    H치 semanas
    &lt;/span&gt;
}
@if (DiasVencidos &gt; 30)
{
    
    if(DiasVencidos &lt; 365)
    {
        &lt;span class="badge badge-danger badge-roundless"&gt;H치 meses&lt;/span&gt;
    }
    if(DiasVencidos &gt; 365 &amp;&amp; DiasVencidos &lt; 730)
    {
        &lt;span class="badge badge-danger badge-roundless"&gt;H치 um ano&lt;/span&gt;
    }
    if(DiasVencidos &gt; 730)
    {
        &lt;span class="badge badge-danger badge-roundless"&gt;H치 anos&lt;/span&gt;
    }
}
@if (DiasVencidos == 1)
{
    &lt;span class="badge badge-info badge-roundless"&gt;
        @DiasVencidos dia
    &lt;/span&gt;
}
@if (DiasVencidos &gt; 1 &amp;&amp; DiasVencidos &lt; 15)
{
    &lt;span class="badge badge-info badge-roundless"&gt;
        @DiasVencidos dias
    &lt;/span&gt;
}

</Value>
