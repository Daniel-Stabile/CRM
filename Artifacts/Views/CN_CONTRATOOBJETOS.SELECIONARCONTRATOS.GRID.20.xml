        <visibility>Visible</visibility>
        <Expression>SELECT SUM(X.SALDO) SQLSALDORECEBER FROM CP_ORDENSCOMPRAENTREGA X WHERE X.HANDLE IN(SELECT B.ORDEMCOMPRAENTREGA FROM CN_OBJETOORDEMCOMPRAENTREGA B WHERE B.OBJETO = @campo(HANDLE)) AND (X.ORDENSCOMPRAITENS IN(SELECT C.HANDLE FROM CP_ORDENSCOMPRAITENS C WHERE C.ORDEMCOMPRA IN(SELECT D.HANDLE FROM CP_ORDENSCOMPRA D WHERE D.STATUS IN(2,3))))</Expression>
