        <Value>@{var DiasVencidos = (DateTime.Now.Date-Convert.ToDateTime(Model.Fields["DataVencimento"]).Date).TotalDays;}
@if (DiasVencidos &lt;= 0) 
{
    &lt;label class="label label-success"&gt;
    Sem atraso
    &lt;/label&gt;
}
@if (DiasVencidos &gt;=15 &amp;&amp; DiasVencidos &lt;= 30)
{
    &lt;label class="label label-warning"&gt;
    H치 semanas
    &lt;/label&gt;
}
@if (DiasVencidos &gt; 30)
{
    
    if(DiasVencidos &lt; 365)
    {
        &lt;label class="label label-danger"&gt;H치 meses&lt;/label&gt;
    }
    if(DiasVencidos &gt; 365 &amp;&amp; DiasVencidos &lt; 730)
    {
        &lt;label class="label label-danger"&gt;H치 um ano&lt;/label&gt;
    }
    if(DiasVencidos &gt; 730)
    {
        &lt;label class="label label-danger"&gt;H치 anos&lt;/label&gt;
    }
}
@if (DiasVencidos == 1)
{
    &lt;label class="label label-info"&gt;
        @DiasVencidos dia
    &lt;/label&gt;
}
@if (DiasVencidos &gt; 1 &amp;&amp; DiasVencidos &lt; 15)
{
    &lt;label class="label label-info"&gt;
        @DiasVencidos dias
    &lt;/label&gt;
}

</Value>
