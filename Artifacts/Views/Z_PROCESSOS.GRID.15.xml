      <GridFieldDefinition xsi:type="GridFieldCustomDefinition" title="Status" key="CUSTOM_PROGRESSO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required xsi:nil="true" />
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Value>@{
    string situacao = Model["SITUACAO"].GetInt32().ToString();
    string progressClass = "default";
    int posicaoFinal = Model["VALORFINAL"].GetInt32();
    if(posicaoFinal &lt;= 0){
        posicaoFinal = 100;
    }
    int posicao = (100 * Model["POSICAO"].GetInt32())/posicaoFinal;
    if ((situacao == "2" || situacao == "3" || situacao == "4" || situacao == "5") &amp;&amp; posicao == 0){
        posicao = 100;
    }
    switch(situacao)
    {
        case "1":
            progressClass = "info";
        break;
        case "2":
            progressClass = "warning";
            break;
        case "3":
            progressClass = "warning";
            break;
        case "4":
            progressClass = "danger";
            break;
        case "5":
            progressClass = "success";
            break;
        case "6":
            progressClass = "default";
            break;            
        default:
            break;                   
    }
}

&lt;div title="@Model["SITUACAO"].GetString()" class='progress' style="margin-bottom:0px"&gt;
    &lt;div class='progress-bar progress-bar-@progressClass' role='progressbar' aria-valuenow='@Model["POSICAO"].GetInt32()' aria-valuemin='@Model["VALORINICIAL"].GetInt32()' aria-valuemax=@posicaoFinal style='width:@posicao%'&gt;
    &lt;/div&gt;
&lt;/div&gt;</Value>
