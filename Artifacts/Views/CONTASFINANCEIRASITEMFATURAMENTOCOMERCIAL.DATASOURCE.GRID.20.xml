    var projeto = Model.Fields["PROJETO"] != null ? Model.Fields["PROJETO"] : "N/A";
    var estrutura = Model.Fields["ESTRUTURACC"];
}
&lt;div class="row static-info grid-retencoes"&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelper("Centro Custo", estrutura + "-" + centroCusto)
