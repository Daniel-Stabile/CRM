    var valorCC = Model.Fields["VALORCC"] != null ? Model.Definition.FieldDefinitions["VALORCC"].ToUI(Model["VALORCC"].Value) : "N/A";
    var percLancamento = Model.Fields["PERCLANCAMENTO"] != null ? Model.Fields["PERCLANCAMENTO"] + "%" : "N/A";
    var percCC = Model.Fields["PERCCC"] != null ? Model.Fields["PERCCC"] + "%" : "N/A";
    var classe = Model.Fields["NATUREZA"] == "C" ? "badge-info" : "badge-danger";   
}
&lt;div class="row static-info grid-retencoes"&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores(classe,"Valor", valor)
    &lt;/div&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores("label-tesouraria-empty","Percentual", percLancamento)
    &lt;/div&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores(classe,"Valor CC", valorCC)
    &lt;/div&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores("label-tesouraria-empty","Percentual CC", percCC)
    &lt;/div&gt;
&lt;/div&gt;</Value>
