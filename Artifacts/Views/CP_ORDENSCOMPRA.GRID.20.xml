      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Status Pedido Empenho" key="STATUSPEDIDOEMPENHODADOS">
        <visible>false</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>false</searchable>
        <visibility>ToScript</visibility>
        <Expression>CASE WHEN EXISTS (SELECT 1 FROM CT_PEDIDOSEMPENHOS WHERE ORDEMCOMPRA = A.HANDLE) THEN CASE WHEN EXISTS (SELECT 1 FROM CT_PEDIDOSEMPENHOS WHERE STATUS IN (1, 6, 7) AND ORDEMCOMPRA = A.HANDLE) THEN 'PENDENTE' ELSE 'FINALIZADO' END ELSE 'SEMPEDIDOEMPENHO' END</Expression>
