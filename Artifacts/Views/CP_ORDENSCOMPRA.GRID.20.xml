      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Total Recebido" key="SQLTOTALRECEBIDO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width>200px</Width>
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>SELECT SUM(CM_ITENS.VALORLIQUIDO)                                      FROM CP_ORDENSCOMPRA                                      JOIN CP_RECEBIMENTOFISICO ON CP_RECEBIMENTOFISICO.ORDEMCOMPRA = CP_ORDENSCOMPRA.HANDLE                                      JOIN CM_ITENS ON  CM_ITENS.HANDLERECEBIMENTOFISICO = CP_RECEBIMENTOFISICO.HANDLE                                      JOIN FN_DOCUMENTOS ON CM_ITENS.DOCUMENTO = FN_DOCUMENTOS.HANDLE                                      JOIN CP_ORDENSCOMPRAITENS ON CP_ORDENSCOMPRAITENS.ORDEMCOMPRA = CP_ORDENSCOMPRA.HANDLE AND CP_ORDENSCOMPRAITENS.HANDLE = CP_RECEBIMENTOFISICO.ORDEMCOMPRAITEM                                     WHERE FN_DOCUMENTOS.STATUS = 2  AND CP_ORDENSCOMPRA.HANDLE = @CAMPO(HANDLE)</Expression>
        <FieldType>Currency</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </GridFieldDefinition>
