      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Comprador (cotação)" key="SQL_COMPRADORCOTACAO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width>200px</Width>
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>SELECT APELIDO FROM (   SELECT DISTINCT Z_GRUPOUSUARIOS.APELIDO, CP_ORDENSCOMPRA.HANDLE FROM CP_REQUISICAOAGRUPADAS   JOIN CP_REQUISICAOCOTACOES ON CP_REQUISICAOAGRUPADAS.REQUISICAOPRODUTO = CP_REQUISICAOCOTACOES.COTACAO   JOIN CP_REQUISICOES ON CP_REQUISICOES.HANDLE = CP_REQUISICAOAGRUPADAS.REQUISICAOHANDLE   JOIN CP_ORDENSCOMPRAITENS ON CP_REQUISICAOCOTACOES.HANDLE = CP_ORDENSCOMPRAITENS.COTACAO   JOIN CP_ORDENSCOMPRA ON CP_ORDENSCOMPRA.HANDLE = CP_ORDENSCOMPRAITENS.ORDEMCOMPRA   JOIN Z_GRUPOUSUARIOS ON CP_REQUISICOES .COMPRADOR = Z_GRUPOUSUARIOS.HANDLE ) CONSULTA_COMPRADOR WHERE HANDLE = @CAMPO(HANDLE)</Expression>
        <FieldType>String</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </GridFieldDefinition>
