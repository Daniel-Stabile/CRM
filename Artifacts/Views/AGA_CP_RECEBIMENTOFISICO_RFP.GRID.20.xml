        <Items>
          <Command title="Selecionar OC" key="CMD_SELECIONAR_OC">
            <visible>true</visible>
            <showModal>false</showModal>
            <modalSize xsi:nil="true" />
            <showInAllLevelsStructuredTable>false</showInAllLevelsStructuredTable>
            <runInEditMode xsi:nil="true" />
            <context>Table</context>
            <OutputDefinition xsi:type="GridLinkDefinition">
              <Url>~/aga/a/materiais/recebimentoFisico/selecionarOC.aspx</Url>
              <IsModalPage>false</IsModalPage>
              <IsModalNavigation>false</IsModalNavigation>
              <WhereClause>
                <Where>(A.SALDO * (SELECT VALORUNITARIO FROM CP_ORDENSCOMPRAITENS WHERE HANDLE = A.ORDENSCOMPRAITENS) &gt; 0 )  AND A.ORDENSCOMPRAITENS IN  (SELECT HANDLE FROM CP_ORDENSCOMPRAITENS WHERE HANDLE NOT IN  (SELECT COALESCE(RF.ORDEMCOMPRAITEM, 0)  FROM CP_RECEBIMENTOFISICO RF, PD_PRODUTOS PROD  WHERE RF.RECEBIMENTOFISICOPAI = @RASTRO(CP_RECEBIMENTOFISICOPAI)  AND PROD.HANDLE = RF.PRODUTO  AND (RF.ALMOXARIFADO = A.ALMOXARIFADO OR PROD.TIPO IN(4, 3)))  AND ORDEMCOMPRA IN  (SELECT HANDLE FROM CP_ORDENSCOMPRA WHERE STATUS IN (2, 3)  AND FORNECEDOR = (SELECT FORNECEDOR FROM CP_RECEBIMENTOFISICOPAI WHERE HANDLE = @RASTRO(CP_RECEBIMENTOFISICOPAI)) AND EMPRESA = (SELECT EMPRESA FROM CP_RECEBIMENTOFISICOPAI WHERE HANDLE = @RASTRO(CP_RECEBIMENTOFISICOPAI)) AND FILIAL = (SELECT FILIAL FROM CP_RECEBIMENTOFISICOPAI WHERE HANDLE = @RASTRO(CP_RECEBIMENTOFISICOPAI)))) </Where>
                <Parameters />
              </WhereClause>
              <SystemInstanceName>CORPORATIVO</SystemInstanceName>
              <TargetEntityDefinitionName>CP_ORDENSCOMPRAENTREGA</TargetEntityDefinitionName>
              <TargetSystemInstanceName>CORPORATIVO</TargetSystemInstanceName>
              <WhereClauseParameters />
              <ParentEntityHandle>
                <NullableValue xsi:nil="true" />
              </ParentEntityHandle>
              <DefaultFilterName />
            </OutputDefinition>
          </Command>
