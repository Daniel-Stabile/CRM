                &lt;li class="default mt-list-item" id="@cacheOrdemSelecionada[0]"&gt;
                    &lt;div class="list-icon-container"&gt;&lt;i class="fa fa-bars"&gt;&lt;/i&gt;&lt;/div&gt;
                    &lt;div class="list-item-content"&gt;@nomesEDescricaoCriterios[(int.Parse(cacheOrdemSelecionada[0])-1)]&lt;/div&gt;
                &lt;/li&gt;
                &lt;li class="default mt-list-item" id="@cacheOrdemSelecionada[1]"&gt;
