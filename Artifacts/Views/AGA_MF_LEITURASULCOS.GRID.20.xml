      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Marca de Fogo" key="SQL_MARCAFOGO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Expression>SELECT RP.MARCAFOGO   FROM MA_RECURSOPARTES AS RP       INNER JOIN MF_MODELOPNEUMEDIDAS B ON (B.HANDLE = RP.MEDIDAS)        INNER JOIN MF_PNEUVIDAS C ON (RP.HANDLE = C.PNEU AND RP.VIDAUTIL = C.VIDA)        INNER JOIN MF_PNEUVIDASULCOS D ON (D.VIDA = C.HANDLE)        LEFT JOIN MF_LEITURASULCOS E ON (E.SULCO = D.HANDLE AND                                         E.DATA = (SELECT MAX(F.DATA)                                                   FROM MF_LEITURASULCOS F                                                 WHERE F.SULCO = D.HANDLE))  WHERE E.HANDLE = A.HANDLE </Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </GridFieldDefinition>
