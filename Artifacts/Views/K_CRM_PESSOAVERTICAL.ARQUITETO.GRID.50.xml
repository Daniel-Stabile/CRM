        <visibility>Visible</visibility>
        <Expression>SELECT CASE WHEN DATEDIFF(DAY, MAX(DATACONCLUSAO), GETDATE()) IS NULL THEN (SELECT DATEDIFF(DAY, MAX(DATAINCLUSAO), GETDATE()) AS QTD FROM K_CRM_PESSOAVERTICAL WHERE PESSOA = @CAMPO(PESSOA) AND VERTICAL = @CAMPO(VERTICAL)) ELSE DATEDIFF(DAY, MAX(DATACONCLUSAO), GETDATE()) END AS DiferencaEmDias  FROM K_CRM_TAREFAUSUARIO WHERE PESSOA = @CAMPO(PESSOA) AND VERTICAL = @CAMPO(VERTICAL) AND (RESPONSAVEL = @CAMPO(USUARIOEXECUTIVO) OR RESPONSAVEL = @CAMPO(USUARIOSDR)) AND STATUS = 2</Expression>
