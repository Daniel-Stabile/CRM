      <FormFieldDefinition xsi:type="FormFieldLabelDefinition" title="Classificação" key="CUSTOM_CLASSIFICACAO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <ValueFromView>@{int statusIndex = Model["SQL_CLASSIFICACAO"].GetInt32();}
    
    
    @if(statusIndex == 1){
        &lt;span class="badge badge-pill badge-primary" style="width: 8rem;height: 2rem;font-size: 1.25rem!important;font-weight: bold;"&gt;Normal&lt;/span&gt;
    }
    else if(statusIndex == 2){
        &lt;span class="badge badge-pill badge-danger" style="height: 2rem;font-size: 1.25rem!important;font-weight: bold;"&gt;Importante&lt;/span&gt;
    }
    else if (statusIndex == 3){
        &lt;span class="badge badge-pill badge-warning" style="height: 2rem;font-size: 1.25rem!important;font-weight: bold;"&gt;Estratégico&lt;/span&gt;
    }
    else{
        &lt;span class="badge badge-pill" style="height: 2rem;font-size: 1.25rem!important;font-weight: bold;"&gt;Nenhuma&lt;/span&gt;
    }
    

    </ValueFromView>
