      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="TOTALAJUSTES" key="TOTALAJUSTES">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression> ( SELECT COALESCE(SUM(ABS(E.VALOR)), 0)  FROM        CT_ORCAMENTOS E WHERE        E.ACOESPROJETOS = A.ACOESPROJETOS        AND E.VERSAO = A.VERSAO        AND E.ORIGEM IN ('C','E','1','6')         ) -       ( SELECT COALESCE(SUM(ABS(E.VALOR)), 0)  FROM       CT_ORCAMENTOS E WHERE       E.ACOESPROJETOS = A.ACOESPROJETOS       AND E.VERSAO = A.VERSAO       AND E.ORIGEM IN ('B','D','Q','5')       )  </Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="TOTALPEDIDOEMPENHO" key="TOTALPEDIDOEMPENHO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression> SELECT             COALESCE(SUM(E.VALOR), 0)         FROM             CT_PEDIDOSEMPENHOS E         WHERE             E.ACOESPROJETOS = A.ACOESPROJETOS             AND E.VERSAO = A.VERSAO             AND E.STATUS IN (2, 6)</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="TOTALEMPENHO" key="TOTALEMPENHO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression>   SELECT             COALESCE(SUM(E.VALOR), 0)         FROM             CT_EMPENHOS E         WHERE             E.ACOESPROJETOS = A.ACOESPROJETOS             AND E.VERSAO = A.VERSAO             AND E.STATUS IN (1)</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
      <FormFieldDefinition key="VALOR">
        <visible>false</visible>
        <DataAccessLevel>Edit</DataAccessLevel>
        <Required xsi:nil="true" />
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
      </FormFieldDefinition>
      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="SALDOEMPENHO" key="SALDOEMPENHO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression>   SELECT             COALESCE(                 SUM(E.VALOR - E.VALORUTILIZADO - E.VALORENCERRADO),                 0             )         FROM             CT_EMPENHOS E         WHERE             E.ACOESPROJETOS = A.ACOESPROJETOS             AND E.VERSAO = A.VERSAO AND E.STATUS = 2</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="TOTALPAGO" key="TOTALPAGO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression>      SELECT             COALESCE(SUM(E.VALORUTILIZADO), 0)         FROM             CT_PEDIDOSEMPENHOS E         WHERE             E.ACOESPROJETOS = A.ACOESPROJETOS             AND E.VERSAO = A.VERSAO</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="TOTALRESERVAR" key="TOTALRESERVAR">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression> SELECT             COALESCE(A.VALOR, 0) - COALESCE(SUM(E.VALOR), 0)         FROM             CT_PEDIDOSEMPENHOS E         WHERE             E.ACOESPROJETOS = A.ACOESPROJETOS             AND E.VERSAO = A.VERSAO</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="TOTALLIQUIDADO" key="TOTALLIQUIDADO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression>  SELECT             COALESCE(SUM(E.VALORNOMINAL), 0)         FROM             FN_DOCUMENTOS E         WHERE             @DATAPARTE(ANO,E.DATAENTRADA) = (                 SELECT @DATAPARTE(ANO,P.ANO)                  FROM OP_ACOESPERIODO P                   WHERE P.HANDLE = A.PERIODO                   )                    AND  E.HANDLE IN ( SELECT L.DOCUMENTO FROM FN_LANCAMENTOS L                     LEFT JOIN FN_LANCAMENTOCC LCC ON LCC.LANCAMENTO = L.HANDLE                     WHERE  E.ABRANGENCIA IN ('R','N') AND E.ENTRADASAIDA = 'E'                     AND  (L.ACOESPROJETOS = A.ACOESPROJETOS                     OR LCC.ACOESPROJETOS = A.ACOESPROJETOS )                )</Expression>
        <FieldType>Float</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </FormFieldDefinition>
    </FieldDefinitions>
