        <ValueFromView>@{var statusNfe=Model.Fields["STATUSNFE"];
var status=Model.Fields["STATUS"];
var nomeStatus = Model.Definition.FieldDefinitions["STATUSNFE"].ToUI(Model["STATUSNFE"].Value);
if ((status == 1) || (status == 4) || (status == 5))
{
    return;
}

if (statusNfe == 1) 
{
    &lt;span class="label label-sm label-info" style="background-color: #3330B5;"&gt;@nomeStatus&lt;/span&gt;
}
if ((statusNfe == 2) || (statusNfe == 3) || (statusNfe == 4))
{
    &lt;span class="label label-sm label-danger" style="background-color: #FFC923;"&gt;@nomeStatus&lt;/span&gt;
}
if ((statusNfe == 5) || (statusNfe == 7) || (statusNfe == 8))
{
    &lt;span class="label label-sm label-success" style="background-color: #CD2024;"&gt;@nomeStatus&lt;/span&gt;
}
if (statusNfe == 6) 
{
    &lt;span class="label label-sm label-success" style="background-color: #21A81A;"&gt;@nomeStatus&lt;/span&gt;
}
if ((statusNfe == 9) || (statusNfe == 10))
{
    &lt;span class="label label-sm label-success" style="background-color: #FFA500;"&gt;@nomeStatus&lt;/span&gt;
}
}</ValueFromView>
