        <where>A.HANDLE IN (SELECT UNIDADE.HANDLE  FROM CM_UNIDADESMEDIDA UNIDADE WHERE UNIDADE.HANDLE IN (SELECT UNIDADE.HANDLE                            FROM CM_UNIDADESMEDIDA UNIDADE                                 JOIN PD_PRODUTOS PRODUTO ON (PRODUTO.UNIDADEMEDIDACOMPRAS = UNIDADE.HANDLE OR PRODUTO.UNIDADEMEDIDAESTOQUE = UNIDADE.HANDLE OR PRODUTO.UNIDADEMEDIDAVENDAS = UNIDADE.HANDLE OR PRODUTO.UNIDADEMEDIDACONSUMO = UNIDADE.HANDLE OR PRODUTO.UNIDADEPRODUCAO = UNIDADE.HANDLE)                                 JOIN CP_REQUISICAOPRODUTOS REQUISICAO ON (REQUISICAO.PRODUTO = PRODUTO.HANDLE)                                 JOIN CP_WEB_ITENSCOTACAO ITEMCOTACAO ON (REQUISICAO.HANDLE = ITEMCOTACAO.REQUISICAOPRODUTO)                           WHERE REQUISICAO.HANDLE = @CAMPO(REQUISICAOPRODUTO)                         )       OR UNIDADE.HANDLE IN (SELECT FATORCONVERSAO.UNIDADE                              FROM PD_PRODUTOFATORESCONVERSAO FATORCONVERSAO                                   JOIN PD_PRODUTOS PRODUTO ON (FATORCONVERSAO.PRODUTO = PRODUTO.HANDLE)                                   JOIN CP_REQUISICAOPRODUTOS REQUISICAO ON (REQUISICAO.PRODUTO = PRODUTO.HANDLE)                             WHERE REQUISICAO.HANDLE = @CAMPO(REQUISICAOPRODUTO)                           )       OR UNIDADE.HANDLE IN (SELECT FATORCONVERSAO.UNIDADE                              FROM PD_PRODUTOFATORESCONVERSAO FATORCONVERSAO                                   JOIN PD_PRODUTOS PRODUTO ON (FATORCONVERSAO.PRODUTO = PRODUTO.HANDLE)                                   JOIN CP_REQUISICAOPRODUTOS REQUISICAO ON (REQUISICAO.PRODUTO = PRODUTO.HANDLE)                             WHERE REQUISICAO.HANDLE = @CAMPO(REQUISICAOPRODUTO)                           ))</where>
