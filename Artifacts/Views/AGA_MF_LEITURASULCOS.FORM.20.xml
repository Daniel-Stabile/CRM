      <FormFieldDefinition xsi:type="FormFieldSqlDefinition" title="Marca Fogo" key="SQL_MARCAFOGO">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <renderTitle>true</renderTitle>
        <Expression>SELECT RP.MARCAFOGO    FROM MA_RECURSOPARTES RP    INNER JOIN MF_MODELOPNEUMEDIDAS B ON (B.HANDLE = RP.MEDIDAS)         INNER JOIN MF_PNEUVIDAS C ON (RP.HANDLE = C.PNEU AND RP.VIDAUTIL = C.VIDA)         INNER JOIN MF_PNEUVIDASULCOS D ON (D.VIDA = C.HANDLE)         LEFT JOIN MF_LEITURASULCOS E ON (E.SULCO = D.HANDLE AND                                  E.DATA = (SELECT MAX(F.DATA)                                            FROM MF_LEITURASULCOS F                                          WHERE F.SULCO = D.HANDLE))        WHERE RP.HANDLE = A.PNEU    AND E.HANDLE = A.HANDLE</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>FROTAS</SystemInstanceName>
      </FormFieldDefinition>
