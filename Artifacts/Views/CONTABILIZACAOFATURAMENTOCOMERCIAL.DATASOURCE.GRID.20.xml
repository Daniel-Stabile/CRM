        <Value>@{
    var valor = Model.Definition.FieldDefinitions["VALORLANCAMENTO"].ToUI(Model["VALORLANCAMENTO"].Value);
    var valorCC = Model.Fields["VALORCC"] != null ? Model.Definition.FieldDefinitions["VALORCC"].ToUI(Model["VALORCC"].Value) : "N/A";
    var percentual = Model.Fields["PERCENTUAL"] != null ? Model.Fields["PERCENTUAL"] + "%" : "N/A";
    var classe = Model.Fields["CONTADEBITO"] != null ? "badge-info" : "badge-danger";   
}
&lt;div class="row static-info grid-retencoes"&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores(classe,"Valor", valor)
    &lt;/div&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores(classe,"Valor CC", valorCC)
    &lt;/div&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelperValores("label-tesouraria-empty","Percentual CC", percentual)
    &lt;/div&gt;
&lt;/div&gt;</Value>
