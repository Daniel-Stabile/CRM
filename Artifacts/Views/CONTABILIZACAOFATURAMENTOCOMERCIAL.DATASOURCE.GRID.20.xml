        <Value>
@{
    var chave = Model.Fields["CHAVE"];
    var documento = Model.Fields["DOCUMENTOCONTABIL"];
    string data = Model.Fields["DATA"] == null ? "" : ((DateTime)Model.Fields["DATA"]).ToShortDateString();
}
&lt;div class="row static-info grid-retencoes" style="display: flex;"&gt;
    &lt;div class="col-lg-5"&gt;
        &lt;div class="col-lg-12"&gt;
            @CorporativoComercialHelper.LabelValueHelper("Chave", chave.ToString())
        &lt;/div&gt;
        &lt;div class="col-lg-12"&gt;
            @CorporativoComercialHelper.LabelValueHelper("Data", data)
        &lt;/div&gt;
    &lt;/div&gt;
    &lt;div class="col-lg-7" style="margin-top: auto; margin-bottom: auto;"&gt;
        &lt;div class="col-lg-12"&gt;
            @CorporativoComercialHelper.LabelValueHelper("Documento", documento)
        &lt;/div&gt;
    &lt;/div&gt;
&lt;/div&gt;</Value>
