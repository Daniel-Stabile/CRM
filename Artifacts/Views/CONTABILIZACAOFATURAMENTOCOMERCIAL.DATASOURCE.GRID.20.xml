        <Value>@{
    var creditoDebito = "";    
    var conta = "";
    if (Model.Fields["CONTADEBITO"] != null)
    {
        creditoDebito = "Débito";
        conta = Model.Fields["CONTADEBITO"];
    }
    else
    {
        creditoDebito = "Crédito";
        conta = Model.Fields["CONTACREDITO"];
    }
}
&lt;div class="row static-info grid-retencoes"&gt;
    &lt;div class="col-lg-12"&gt;
        @CorporativoComercialHelper.LabelValueHelper(creditoDebito, conta)
    &lt;/div&gt;
&lt;/div&gt;</Value>
