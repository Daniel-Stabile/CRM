      <GridFieldDefinition xsi:type="GridFieldSqlDefinition" title="Possui catálogo" key="SQL_636682190770438582">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required>false</Required>
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable xsi:nil="true" />
        <visibility>Visible</visibility>
        <Expression>SELECT CASE WHEN A.QTD &gt; 0 THEN 'Sim' ELSE 'Não' END FROM (SELECT COUNT(X.HANDLE) QTD FROM CP_CATALOGOFORNECIMENTOITENS X INNER JOIN CP_CATALOGOSFORNECIMENTO W ON W.HANDLE = X.CATALOGOFORNECIMENTO INNER JOIN CP_CATALOGOFORNECIMENTOFILIAIS Z ON Z.CATALOGOFORNECIMENTO = X.CATALOGOFORNECIMENTO  WHERE W.INICIOVIGENCIA &lt;= @HOJE AND W.FINALVIGENCIA &gt;= @HOJE AND X.PRODUTOPAI = @CAMPO(HANDLE) AND Z.FILIAL IN @FILIAIS ) A</Expression>
        <FieldType>None</FieldType>
        <SystemInstanceName>CORPORATIVO</SystemInstanceName>
      </GridFieldDefinition>
