        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Value>@{
    var vcto = @Model["VENCIMENTOORIGINAL"].GetDateTime() &lt;= DateTime.MinValue;
}
@if (vcto)
{
    &lt;p&gt;@Model["DATAVENCIMENTO"].GetDateTime().ToShortDateString()&lt;/p&gt;
}
else
{
    &lt;p&gt;@Model["VENCIMENTOORIGINAL"].GetDateTime().ToShortDateString()&lt;/p&gt;
}</Value>
