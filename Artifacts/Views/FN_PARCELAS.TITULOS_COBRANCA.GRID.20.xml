        <Value>@{
    var hoje = DateTime.Now - @Model["VCTOPRORROGADO"].GetDateTime();
}
@if (hoje.Days &lt;= 0)
{
    &lt;p&gt;0&lt;/p&gt;
}
else
{
    var valorVencido = @Model["VALOR"].GetDecimal() - @Model["VALORESBAIXADOS"].GetDecimal();
    &lt;p style="color:lightcoral"&gt;@valorVencido.ToString("C")&lt;/p&gt;
}</Value>
