      <GridFieldDefinition xsi:type="GridFieldCustomDefinition" title="Dias atraso" cssClasses="text-center" key="CUSTOM_DIAS">
        <visible>true</visible>
        <DataAccessLevel>Read</DataAccessLevel>
        <Required xsi:nil="true" />
        <HasScriptEvent xsi:nil="true" />
        <footerBehavior>None</footerBehavior>
        <Width />
        <searchable>true</searchable>
        <visibility>Visible</visibility>
        <Value>@{
    var hoje = DateTime.Now - @Model["VCTOPRORROGADO"].GetDateTime();
}
@if (hoje.Days &lt;= 0)
{
    &lt;p&gt;0&lt;/p&gt;
}
else
{
    &lt;p style="color:lightcoral"&gt;@hoje.Days&lt;/p&gt;
}</Value>
        <ValueType>HTML</ValueType>
      </GridFieldDefinition>
