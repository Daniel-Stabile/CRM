          <Command title="Anexos capa da cotação" image="fa fa-paperclip" color="blue" key="CMD_ANEXOSCAPACOTACAO">
            <visible>true</visible>
            <showModal>true</showModal>
            <modalSize>Large</modalSize>
            <showInAllLevelsStructuredTable>false</showInAllLevelsStructuredTable>
            <runInEditMode>false</runInEditMode>
            <context>Record</context>
            <OutputDefinition xsi:type="GridLinkDefinition">
              <Url>~/aga/a/Suprimentos/cotacoes/AnexosCotacao.aspx</Url>
              <IsModalPage>false</IsModalPage>
              <IsModalNavigation>false</IsModalNavigation>
              <WhereClause>
                <Where>A.HANDLE IN ( SELECT DOCTOCAPA.HANDLE FROM CP_WEB_DOCUMENTOSCOTACAO DOCTOCAPA JOIN CP_WEB_COTACOES WEBCOTACAO ON DOCTOCAPA.COTACAO = WEBCOTACAO.HANDLE JOIN CP_WEB_ITENSCOTACAO WEBITEMCOTACAO ON WEBITEMCOTACAO.COTACAO = WEBCOTACAO.HANDLE JOIN CP_REQUISICAOCOTACOES REQCOT ON REQCOT.COTACAO = WEBITEMCOTACAO.REQUISICAOPRODUTO JOIN CP_WEB_COTACAORODADAS RODADA ON RODADA.HANDLE = WEBITEMCOTACAO.RODADA AND RODADA.NUMERORODADA = REQCOT.RODADA WHERE REQCOT.HANDLE = @CAMPO(HANDLE))</Where>
                <Parameters />
              </WhereClause>
              <SystemInstanceName>CORPORATIVO</SystemInstanceName>
              <TargetEntityDefinitionName>CP_WEB_DOCUMENTOSCOTACAO</TargetEntityDefinitionName>
              <TargetSystemInstanceName>CORPORATIVO</TargetSystemInstanceName>
              <WhereClauseParameters />
              <ParentEntityHandle>
                <NullableValue xsi:nil="true" />
              </ParentEntityHandle>
              <DefaultFilterName />
            </OutputDefinition>
          </Command>
          <Command title="Anexos item da cotação" image="fa fa-paperclip" color="blue" key="CMD_ANEXOSITEMCOTACAO">
            <visible>true</visible>
            <showModal>true</showModal>
            <modalSize>Large</modalSize>
            <showInAllLevelsStructuredTable>false</showInAllLevelsStructuredTable>
