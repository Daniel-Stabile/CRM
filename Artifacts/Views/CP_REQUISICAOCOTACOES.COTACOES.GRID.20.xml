              <IsModalPage>false</IsModalPage>
              <IsModalNavigation>false</IsModalNavigation>
              <WhereClause>
                <Where>A.HANDLE= ( SELECT  DOCTOITEM.HANDLE   FROM CP_WEB_DOCUMENTOSCOTACAO DOCTOITEM         JOIN CP_WEB_ITENSCOTACAO WEBITEMCOTACAO ON DOCTOITEM.COTACAOITEM = WEBITEMCOTACAO.HANDLE         JOIN CP_REQUISICAOCOTACOES REQCOT ON REQCOT.COTACAO = WEBITEMCOTACAO.REQUISICAOPRODUTO         JOIN CP_WEB_COTACAORODADAS RODADA ON RODADA.HANDLE = WEBITEMCOTACAO.RODADA AND RODADA.NUMERORODADA = REQCOT.RODADA  WHERE REQCOT.HANDLE  = @CAMPO(HANDLE))</Where>
                <Parameters />
              </WhereClause>
