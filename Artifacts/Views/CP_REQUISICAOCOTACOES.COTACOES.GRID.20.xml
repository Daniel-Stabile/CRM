          <Command title="Entregas específicas" image="fa fa-calendar" color="green-seagreen" key="CMD_ENTREGAESPECIFICAAGRUPADA">
            <visible>true</visible>
            <showModal>true</showModal>
            <modalSize>Large</modalSize>
            <showInAllLevelsStructuredTable>false</showInAllLevelsStructuredTable>
            <runInEditMode>false</runInEditMode>
            <context>Record</context>
            <OutputDefinition xsi:type="GridLinkDefinition">
              <Url>~/aga/a/suprimentos/Cotacoes/EntregasEspecificasAgrupada.aspx?HANDLE=@CAMPO(HANDLE)</Url>
              <IsModalPage>false</IsModalPage>
              <IsModalNavigation>false</IsModalNavigation>
              <WhereClause>
                <Where>A.HANDLE IN (SELECT A.HANDLE   FROM CP_REQUISICOES A,        CP_REQUISICAOAGRUPADAS B  WHERE A.HANDLE = B.REQUISICAOHANDLE        AND B.REQUISICAOPRODUTO =  (SELECT HANDLE  FROM CP_REQUISICAOPRODUTOS                  WHERE HANDLE = (SELECT COTACAO FROM CP_REQUISICAOCOTACOES              WHERE HANDLE = @CAMPO(HANDLE) ) ) )</Where>
                <Parameters />
              </WhereClause>
              <SystemInstanceName>CORPORATIVO</SystemInstanceName>
