                                                                       WHERE MAXDATA.ORIGEMCONTAGERENCIAL = MAXCONTA.ORIGEMCONTAGERENCIAL
                                                                             AND MAXDATA.VERSAO = MAXCONTA.VERSAO
                                                                             AND MAXDATA.EMPRESA = MAXCONTA.EMPRESA
                                                                             AND MAXDATA.DATAINCLUSAO &lt; PERIODO.DATAINICIAL
                                                             )
                                             )
                                         )
                                 )
                             ) 

