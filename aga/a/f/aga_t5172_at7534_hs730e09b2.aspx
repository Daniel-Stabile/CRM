                                                         WHERE MAXCONTA.ORIGEMCONTA = CONTA.ORIGEMCONTA
                                                               AND MAXCONTA.VERSAO = CONTA.VERSAO
                                                               AND MAXCONTA.EMPRESA = CONTA.EMPRESA
                                                               AND MAXCONTA.DATAINCLUSAO &lt; PERIODO.DATAINICIAL
                                                               AND MAXCONTA.DATAINCLUSAO =
